{
    "name": "days-syntax",
    "displayName": "Days Syntax",
    "description": "Syntax highlighting for calorie tracking input file format.",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.47.0"
    },
    "publisher": "ChrisRedford",
    "repository": "https://github.com/drgrib/days-syntax",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "days",
                "aliases": [
                    "days",
                    "days"
                ],
                "extensions": [
                    ".days"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "days",
                "scopeName": "source.days",
                "path": "./syntaxes/days.tmLanguage.json"
            }
        ]
    }
}